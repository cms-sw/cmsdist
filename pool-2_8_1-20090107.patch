+{
+}
+
+bool
+pool::RelationalStorageService::ObjectUpdater::update( int oid,
+                                                       const void* data,
+                                                       pool::DataCallBack& callback )
+{
+  return update(oid,dataBuffer(),data,callback);
+}
+
+bool
+pool::RelationalStorageService::ObjectUpdater::update( int oid,
+                                                       ObjectRelationalDataBuffer& dataBuff,
+                                                       const void* data,
+                                                       pool::DataCallBack& callback )
+{
+  coral::AttributeList& buffer = dataBuff.data();
+
+  // Update first the row of the top level table
+  coral::AttributeList::iterator iAttribute = buffer.begin();
+
+  // Skip the identity columns
+  for ( size_t iColumn = 0; iColumn < dataBuff.mapping().columnNames().size(); ++iColumn ) ++iAttribute;
+
+  // Copy the data
+  std::vector<int>::const_iterator iOffset = dataBuff.offsets().begin();
+  while ( iAttribute != buffer.end() ) {
+    int offset = *iOffset;
+    if ( offset < 0 ) {
+      pool::Token::OID_t oid;
+      const void* refAddress = static_cast< const char* >( data ) - ( 1 + offset );
+      // Call here the ref handler to get the oid...
+      pool::DbDataHandlerGuard handlerGuard( &callback, m_databaseContext );
+      callback.notify( pool::DataCallBack::PUT, pool::DataCallBack::REFERENCE, refDictionary(), const_cast< void* >( refAddress ), &oid );
+      iAttribute->data<int>() =  oid.first;
+      ++iAttribute;
+      ++iOffset;
+      iAttribute->data<int>() =  oid.second;
+    }
+    else {
+      if( iAttribute->specification().type() == typeid(coral::Blob) )
+      {
+        iAttribute->bind<coral::Blob>( dataBuff.writeBlob( offset, static_cast< const char* >( data ) + offset ) );
+      } else 
+      {
+        iAttribute->setValueFromAddress( static_cast< const char* >( data ) + offset );
+      }
+    }
+    ++iAttribute;
+    ++iOffset;
+  }
+
+  // Set the oid. For the moment set only the first identity column. In principle there should be a plugin taking
+  // care of the OID -> identity values mapping.
+  buffer.begin()->data< int >() =  oid;
+
+  // Update the rows into the table
+  dataBuff.table().dataEditor().updateRows( dataBuff.updateClause(), dataBuff.idClause(), buffer );
+
+  // Update the arrays...
+  // now remove the object dependencies...
+  deleteObjectDependencies(dataBuff.mapping(),oid, dataBuff.mapping().tableName());
+  //  re-writing the arrays now...
+  std::vector<int> identity( 1, oid );
+  for ( pool::RelationalStorageService::ObjectRelationalDataBuffer::iterator iBuffer = dataBuff.begin();
+        iBuffer != dataBuff.end(); ++iBuffer ) {
+    if ( ! this->writeArray( identity, **iBuffer,
+                             static_cast< const char* >( data ) + (*iBuffer)->offsetToParent(),
+                             callback ) ) return false;
+  }
+  // Update the pool pointers...
+  for ( std::vector<ObjectRelationalDataBuffer*>::const_iterator iBuffer = dataBuff.poolPointerBuffers().begin();
+        iBuffer != dataBuff.poolPointerBuffers().end(); ++iBuffer ) {
+    if ( ! this->updatePoolPointer( oid, *(*iBuffer),
+                                   static_cast< const char* >( data ) + (*iBuffer)->offsetToParent(),
+                                   callback ) ) return false;
+  }
+  return true;
+}
+
+bool
+pool::RelationalStorageService::ObjectUpdater::updatePoolPointer( int oid,
+                                                                  ObjectRelationalDataBuffer& dataBuff,
+                                                                  const void* data,
+                                                                  pool::DataCallBack& callback )
+{
+  ROOT::Reflex::Object ptrObj(dataBuff.dictionary(),const_cast<void*>(data));
+
+#if ROOT_VERSION_CODE < ROOT_VERSION(5,21,3)
+  bool isNull = *static_cast<bool*>(ptrObj.Invoke("operator!").Address());
+#else
+  bool isNull;
+  ptrObj.Invoke("operator!",isNull);
+#endif
+
+  if(!isNull){
+#if ROOT_VERSION_CODE < ROOT_VERSION(5,21,3)
+    void* ptrData = ptrObj.Invoke("operator*").Address();
+#else
+    void* ptrData;
+    ptrObj.Invoke("operator*",ptrData);
+#endif
+    return update(oid,dataBuff,ptrData,callback);  
+  }
+  return true;
+}
+
+bool
+pool::RelationalStorageService::ObjectUpdater::deleteObjectDependencies( const pool::ObjectRelationalMappingElement& mapping,
+                                                                         int oid, const std::string& topLevelTable )
+{
+  for ( pool::ObjectRelationalMappingElement::const_iterator iMapping = mapping.begin();
+        iMapping != mapping.end(); ++iMapping ) {
+    pool::ObjectRelationalMappingElement::ElementType elementType = iMapping->second.elementType();
+    if ( elementType == pool::ObjectRelationalMappingElement::Object ||
+         elementType == pool::ObjectRelationalMappingElement::Array ||
+         elementType == pool::ObjectRelationalMappingElement::CArray ) {
+      this->deleteObjectDependencies( iMapping->second, oid, topLevelTable );
+    }
+  }
+
+  if ( mapping.tableName()==topLevelTable || mapping.elementType() == pool::ObjectRelationalMappingElement::Object) return true;
+  size_t columnId = mapping.columnNames().size() - 1;
+  std::string oidColumn = mapping.columnNames()[ columnId ];
+  coral::AttributeList condition;
+  condition.extend< int >( oidColumn );
+  condition.begin()->data< int >() = oid;
+  this->m_schema.tableHandle( mapping.tableName() ).dataEditor().deleteRows( oidColumn + " = :" + oidColumn,
+                                                                             condition );
+  return true;
+}

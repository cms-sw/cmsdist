+bool pool::RelationalStorageService::PoolPrimitivesContainerManager::readAnyObjectFromCache( void** data,
+                                                                                             int oid,
+                                                                                             pool::DataCallBack& callback,
+                                                                                             const std::string& type_name,
+                                                                                             const TypeH& dictionary,
+                                                                                             pool::DbDatabase* databaseHandle ){
+  bool found = false;
+  unsigned int cacheMaxSize = 20;
+  m_typeName = type_name;
+  if(m_queryCache.empty()){
+    this->prepareOutputDataBuffer( type_name );
+
+    // Set the oid
+    m_idOnReading->begin()->data<int>() = oid;
+
+    // Create the query
+    std::auto_ptr<coral::IQuery> query(m_schema.tableHandle( m_tableName ).newQuery());
+
+    query->addToOutputList( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable() );
+    query->defineOutputType( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable(), coral::AttributeSpecification::typeNameForType<int>() );
+    query->addToOutputList( pool::RelationalStorageService::RelationalStorageServiceNames::dataVariableNameInPrimitivesContainerTable() );
+    query->defineOutputType( pool::RelationalStorageService::RelationalStorageServiceNames::dataVariableNameInPrimitivesContainerTable(), m_typeName );
+
+    query->setCondition( m_whereClauseForLowId, *m_idOnReading );
+    query->addToOrderList( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable() );
+    query->limitReturnedRows( cacheMaxSize, 0 );
+
+    coral::ICursor& cursor = query->execute();
+    bool first = true;
+    while ( cursor.next() ) {
+      found = true;
+      const coral::AttributeList& row = cursor.currentRow();
+      coral::AttributeList::const_iterator iAttribute = row.begin();
+      int theOid = iAttribute->data< int >();
+      ++iAttribute;
+      if(first){
+        oid = theOid;
+        iAttribute->copyValueToAddress(*data);
+        first = false;
+      }
+      m_queryCache.push_back(std::make_pair(theOid,row));
+    }
+  } else {
+    unsigned int i=0;
+    unsigned int siz = m_queryCache.size();
+    while ( i<siz && !found){
+      if(m_queryCache[i].first>=oid){
+        found = true;
+        const coral::AttributeList& theRow = m_queryCache[i].second;
+        coral::AttributeList::const_iterator iAttribute = theRow.begin();
+        oid = iAttribute->data< int >();
+        ++iAttribute;
+        iAttribute->copyValueToAddress(*data);        
+      }
+      i++;
+    }
+    if(!found ){
+      m_queryCache.clear();
+      if( i==cacheMaxSize ){
+        found = readAnyObjectFromCache(data,oid,callback,type_name,dictionary,databaseHandle);
+      }
+    }
+  }
+  return found;
+}
+
+bool
+pool::RelationalStorageService::PoolPrimitivesContainerManager::readAnyObjectWithNewQuery( void** data,
+                                                                                           int oid,
+                                                                                           pool::DataCallBack&,
+                                                                                           const std::string& type_name,
+                                                                                           const TypeH&,
+                                                                                           pool::DbDatabase* )
+{
+  if ( ! m_inputDataBuffer ) this->prepareInputDataBuffer( type_name );
+  else if ( type_name != m_typeName ) return false;
+  this->prepareOutputDataBuffer( type_name );
+
+  // Set the oid
+  m_idOnReading->begin()->data<int>() = oid;
+
+  // Create the query
+  coral::IQuery* query = m_schema.tableHandle( m_tableName ).newQuery();
+
+  query->addToOutputList( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable() );
+  query->defineOutputType( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable(), coral::AttributeSpecification::typeNameForType<int>() );
+  
+  query->addToOutputList( pool::RelationalStorageService::RelationalStorageServiceNames::dataVariableNameInPrimitivesContainerTable() );
+  query->defineOutputType( pool::RelationalStorageService::RelationalStorageServiceNames::dataVariableNameInPrimitivesContainerTable(), m_typeName );
+  
+  query->setCondition( m_whereClauseForLowId, *m_idOnReading );
+  query->addToOrderList( pool::RelationalStorageService::RelationalStorageServiceNames::idVariableNameInPrimitivesContainerTable() );
+  query->limitReturnedRows( 1, 0 );
+
+  bool success = true;
+  coral::ICursor& cursor = query->execute();
+  if ( cursor.next() ) {
+    const coral::AttributeList& row = cursor.currentRow();
+    coral::AttributeList::const_iterator iAttribute = row.begin();
+    oid = iAttribute->data< int >();
+    ++iAttribute;
+    iAttribute->copyValueToAddress(*data);
+  }
+  else {
+    success = false;
+  }
+
+  delete query;
+  return success;
+}

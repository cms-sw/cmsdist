+  coral::IWebCacheControl& cacheControl = service.webCacheControl();
+  
+  // set the mapping tables
+  std::set<std::string> mappingTables = pool::ObjectRelationalMappingSchema::listTables();
+  for(std::set<std::string>::const_iterator iT=mappingTables.begin();iT!=mappingTables.end();++iT){

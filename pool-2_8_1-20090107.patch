+pool::ObjectRelationalMappingSchema::existTables(){
+  bool ret = false;
+  std::set<std::string> tableList = listTables();
+  size_t nmissing = 0;
+  coral::MessageStream log( pool::ObjectRelationalAccess::serviceName() );
+  try {
+    for(std::set<std::string>::const_iterator iT=tableList.begin(); iT!=tableList.end();++iT){
+      if(!m_schema.existsTable( *iT )){
+        nmissing++;
+      }

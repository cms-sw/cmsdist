+pool::RelationalStorageService::ObjectUpdater::ObjectUpdater( coral::ISchema& schema,
+                                                            const Reflex::Type& dictionary,
+                                                            const pool::ObjectRelationalMappingElement& mapping,
+                                                            pool::DbDatabase* databaseHandle ):
+  DbIOHandler(databaseHandle),
+  ObjectWriter(schema, dictionary, mapping, databaseHandle),
+  m_schema(schema)
+{
+}

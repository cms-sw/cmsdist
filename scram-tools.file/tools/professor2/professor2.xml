<tool name="professor2" version="@TOOL_VERSION@">
<client>
<environment name="PROFESSOR2_BASE" default="@TOOL_ROOT@"/>
<environment name="LIBDIR" default="$PROFESSOR2_BASE/lib"/>
</client>
<use name="py3-numpy"/>
<use name="root"/>
<use name="yoda"/>
<use name="eigen"/>
<runtime name="PATH" value="$PROFESSOR2_BASE/bin" type="path"/>
</tool>
